[global]
repo1-type=s3
repo1-s3-endpoint=${s3_endpoint}
repo1-s3-bucket=${s3_bucket}
repo1-s3-key=${s3_access_key}
repo1-s3-key-secret=${s3_secret_key}
repo1-s3-region=${s3_region}
%{ if s3_force_path_style == true }repo1-s3-uri-style=path
%{ endif }repo1-path=/var/lib/pgbackrest

repo1-retention-full=1
start-fast=y
archive-async=y

[${pgbackrest_stanza}]
pg1-path=/var/lib/postgresql/15/main
